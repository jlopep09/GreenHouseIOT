{
"$schema": "https://schema.up.railway.app/railway.schema.json",
"build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
    "providers": ["python"],
    "phases": {
        "setup": {
        "nixPkgs": ["python3", "pip"]
        }
    }
    }
},
"deploy": {
    "startCommand": "hypercorn app.main:app --bind \"[::]:$PORT\"",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
}
}
  